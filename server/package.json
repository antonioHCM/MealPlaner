{
  "name": "mealplaner-server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "test:dev": "cross-env NODE_ENV=test ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,js}' --fix",
    "typecheck": "tsc -p . --noEmit"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.18.2",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "7.6.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.12.12",
    "cross-env": "7.0.3",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  }
}
